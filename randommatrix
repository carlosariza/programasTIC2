//rellenar matrices de forma aleatoria
#include<iostream>
#include<stdlib>
using namespace std;

int main(){
  int matrix[3][3];
  int fil,col;
  int num;
  for(fil=0;fil<3;fil++){
    for(col=0;col<3;col++){
    num=rand();
    cout<<num<<" ";
      matrix[fil][col]= num;
    
    }
  
  }
return 0;
}
